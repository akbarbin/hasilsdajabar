<h1>Listing posts</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Description</th>
    <th>Harvest date</th>
    <th>Quantity</th>
    <th>Selling price</th>
    <th>Purchase price</th>
    <th>Slug</th>
    <th>User</th>
    <th>Wholesellerid</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @posts.each do |post| %>
  <tr>
    <td><%= post.title %></td>
    <td><%= post.description %></td>
    <td><%= post.harvest_date %></td>
    <td><%= post.quantity %></td>
    <td><%= post.selling_price %></td>
    <td><%= post.purchase_price %></td>
    <td><%= post.slug %></td>
    <td><%= post.user_id %></td>
    <td><%= post.wholesellerid %></td>
    <td><%= link_to 'Show', post %></td>
    <td><%= link_to 'Edit', edit_post_path(post) %></td>
    <td><%= link_to 'Destroy', post, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Post', new_post_path %>
